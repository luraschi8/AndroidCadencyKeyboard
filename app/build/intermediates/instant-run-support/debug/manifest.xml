<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.cadencykeyboard" >

    <uses-sdk android:minSdkVersion="19" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:label="Cadency Keyboard" >
        <service
            android:name="com.example.android.cadencyKeyboard.CadencyKeyboard"
            android:permission="android.permission.BIND_INPUT_METHOD" >
            <intent-filter>
                <action android:name="android.view.InputMethod" />
            </intent-filter>

            <meta-data
                android:name="android.view.im"
                android:resource="@xml/method" />
        </service>

        <activity
            android:name="com.example.android.cadencyKeyboard.ImePreferences"
            android:label="Cadency Keyboard Settings" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>