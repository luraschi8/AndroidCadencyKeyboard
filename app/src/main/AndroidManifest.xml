<manifest xmlns:android="http://schemas.android.com/apk/res/android" 
        package="com.example.android.cadencyKeyboard">
    <application android:label="Cadency Keyboard">
        <service android:name="com.example.android.cadencyKeyboard.CadencyKeyboard"
                android:permission="android.permission.BIND_INPUT_METHOD">
            <intent-filter>
                <action android:name="android.view.InputMethod" />
            </intent-filter>
            <meta-data android:name="android.view.im" android:resource="@xml/method" />
        </service>

        <activity android:name="com.example.android.cadencyKeyboard.ImePreferences" android:label="Cadency Keyboard Settings">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>
</manifest>
